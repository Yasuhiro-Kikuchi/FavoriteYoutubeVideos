<h1>Videoconfigs#edit</h1>
<p>※ブログ設定の編集</p>
<% if @videoconfig.errors.any? %>
<ul>
    <% @videoconfig.errors.full_messages.each do |err| %>
        <li><%= err %></li>
    <% end %>
</ul>
<% end %>
<table>
    <%= form_for(@videoconfig, url:{controller:"videoconfigs",action:"edit"}) do |form| %>
    <tr><th>Titile</th>
        <td><%= form.text_field :title %></td></tr>
    <tr><th>Subtitile</th>
        <td><%= form.text_field :subtitle %></td></tr>
    <tr><th>Style name</th>
        <td><%= form.text_field :stylename %></td></tr>
    <tr><th></th>
        <td><%= form.submit "送信" %></td></tr>
    <% end %>
</table>